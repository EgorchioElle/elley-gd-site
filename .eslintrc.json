{
  "extends": "next/core-web-vitals",
  "rules": {
    "semi": "warn",
    "quotes": ["warn", "single"],
    "jsx-quotes": ["warn", "prefer-double"],
    "prefer-const": "warn",
    "comma-dangle": "warn",
    "react-hooks/rules-of-hooks": "warn",
    "import/order": ["error", {
      "groups": ["builtin", "external", "internal", "parent", "sibling", "index", "object", "type"],
      "newlines-between": "always-and-inside-groups"
    }]
  }
}
